CREATE TABLE `countsals` (
	`person_id` INT(10) NOT NULL AUTO_INCREMENT,
	`price` VARCHAR(50) NULL DEFAULT NULL COLLATE 'cp1250_general_ci',
	`fname` VARCHAR(50) NULL DEFAULT NULL COLLATE 'cp1250_general_ci',
	`lname` VARCHAR(50) NULL DEFAULT NULL COLLATE 'cp1250_general_ci',
	PRIMARY KEY (`person_id`) USING BTREE
)
COLLATE='cp1250_general_ci'
ENGINE=InnoDB
AUTO_INCREMENT=8
;
